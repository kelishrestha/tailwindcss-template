  "bugs": {
    "url": "https://github.com/kelishrestha/tailwindcss-template/issues"
  },
  "homepage": "https://github.com/kelishrestha/tailwindcss-template#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.0"
  },
  "dependencies": {
    "live-server": "^1.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2"
  }
}
