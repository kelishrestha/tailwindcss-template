{
  "name": "tailwindcss-template",
  "description": "Tailwind CSS Template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "postcss src/tailwind.source.css -o public/styles.css",
    "prod-build": "tailwindcss -i src/tailwind.source.css -o public/styles.css --minify",
    "dev": "npm run build && live-server public --port=8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelishrestha/tailwindcss-template.git"
  },
